{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug auth.service",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/services/auth.service/src/build/Debug/auth.service",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/services/auth.service/src",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build auth.service (Debug)",
            "miDebuggerPath": "/usr/bin/gdb",
            "logging": {
                "engineLogging": false
            }
        },
        {
            "name": "Debug prompt.service",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/services/prompt.service/src/build/Debug/prompt.service",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/services/prompt.service/src",
            "environment": [
                {
                    "name": "PROMPT_BIND_ADDR",
                    "value": "0.0.0.0"
                },
                {
                    "name": "PROMPT_BIND_PORT",
                    "value": "2222"
                },
                {
                    "name": "PROMPT_THREADS",
                    "value": "4"
                },
                {
                    "name": "PROMPT_SSH_RSA_HOST_PUBLIC_KEY_PATH",
                    "value": "${workspaceFolder}/services/prompt.service/dev-artifacts/host_rsa.pub"
                },
                {
                    "name": "PROMPT_SSH_RSA_HOST_PRIVATE_KEY_PATH",
                    "value": "${workspaceFolder}/services/prompt.service/dev-artifacts/host_rsa"
                }
